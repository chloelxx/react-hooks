{
  "editor.fontSize": 14,
  "eslint.alwaysShowStatus": true,
  "files.associations": { "*.cjson": "jsonc", "*.wxss": "css", "*.wxs": "javascript" },
  "emmet.includeLanguages": { "wxml": "html" },
  "minapp-vscode.disableAutoConfig": true,
  "editor.tabSize": 2,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": true,
    "suppressLineUncommittedWarning": true,
    "suppressSupportGitLensNotification": true
  },
  "gitlens.views.repositories.files.layout": "list",
  "prettier.useTabs": true,
  "liveServer.settings.CustomBrowser": "chrome",
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "terminal.integrated.rendererType": "dom",
  "git.enableSmartCommit": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "search.followSymlinks": false,
  "prettier.printWidth": 180,
  "vetur.format.defaultFormatterOptions": {
    "prettier": { "semi": false, "singleQuote": true, "trailingComma": false }
  },
  "javascript.format.insertSpaceBeforeFunctionParenthesis": false,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "git-graph.dateType": "Commit Date",
  "git-graph.showCurrentBranchByDefault": true,
  "diffEditor.renderSideBySide": false,
  "liveServer.settings.donotShowInfoMsg": true,
  "gitlens.gitCommands.closeOnFocusOut": true,
  "gitlens.views.repositories.branches.layout": "list",
  "task.slowProviderWarning": ["npm"],
  "editor.codeActionsOnSave": { "source.organizeImports": true, "source.fixAll": true, "source.fixAll.eslint": true },
  "editor.fontWeight": "500",
  "prettier.jsxBracketSameLine": true,
  "prettier.semi": false,
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "editor.renderWhitespace": "all",
  "editor.wordWrapColumn": 120,
  "editor.autoIndent": "none",
  "editor.renderLineHighlight": "all",
  "editor.renderControlCharacters": true,
  "rest-client.decodeEscapedUnicodeCharacters": true,
  "git.autofetch": true,
  "fileheader.Author": "wangxiaofeng",
  "fileheader.LastModifiedBy": "wangxiaofeng",
  "diffEditor.ignoreTrimWhitespace": false,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "[typescriptreact]": { "editor.defaultFormatter": "vscode.typescript-language-features" },
  "[vue]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[scss]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "python.dataScience.askForKernelRestart": false,
  "python.dataScience.interactiveWindowMode": "perFile",
  "editor.codeActionsOnSave": { "source.organizeImports": true, "source.fixAll": true, "source.fixAll.eslint": true }
}
